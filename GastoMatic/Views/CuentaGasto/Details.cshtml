@model GastoMatic.Models.CuentaGastosDetalle

@{
    ViewBag.Title = "Details";
}
<div class="details">

<h2>Details</h2>

<fieldset>
    <legend>CuentaGastos</legend>

    <div class="display-label">IdCuentaGastos</div>
    <div class="display-field">@ViewBag.Model.IdCuentaGastos</div>

    <div class="display-label">Fecha Creación</div>
    <div class="display-field">@String.Format("{0:g}", ViewBag.Model.FechaCreacion)</div>

    <div class="display-label">Fecha Inicial</div>
    <div class="display-field">@String.Format("{0:g}", ViewBag.Model.FechaInicial)</div>

    <div class="display-label">Fecha Final</div>
    <div class="display-field">@String.Format("{0:g}", ViewBag.Model.FechaFinal)</div>

    <div class="display-label">Numero Acreedor</div>
    <div class="display-field">@ViewBag.Model.NumeroAcreedor</div>

    <div class="display-label">Descripción</div>
    <div class="display-field">@ViewBag.Model.Descripcion</div>
</fieldset>
<p>
    @Html.ActionLink("Back to List", "Index")
</p>

</div>